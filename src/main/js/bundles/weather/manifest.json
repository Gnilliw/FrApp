{
    "name": "weather",
    "version": "1.0.0",
    "title": "${bundleName}",
    "description": "${bundleDescription}",
    "vendor": "con terra GmbH",
    "productName": "frapp",
    "dependencies": {
        "esri": "^4.10.0",
        "apprt-vue": "^4.7.0",
        "apprt-vuetify": "^4.7.0"
    },
    "layer": "module",
    "i18n": [
        "bundle"
    ],
    "layout-widgets": [
        {
            "widgetRole": "weatherWidget",
            "window": {
                "title": "weatherWindow",
                "marginBox": {
                    "l": 50,
                    "b": 150,
                    "w": 300,
                    "h": 200
                }
            }
        }
    ],
    "CSS-Themes-Extension": [],
    "components": [
        {
            "name": "weatherFactory",
            "provides": "dijit.Widget",
            "instanceFactory": true,
            "properties": {
                "widgetRole": "weatherWidget"
            }
        }
    ]
}
