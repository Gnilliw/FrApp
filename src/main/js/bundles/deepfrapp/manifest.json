{
    "name": "deepfrapp",
    "version": "1.0.0",
    "title": "DeepFrAppTitel",
    "description": "DeepFrAppBeschreibung",
    "vendor": "con terra GmbH",
    "productName": "frapp",
    "dependencies": {
        "esri": "^4.10.0",
        "apprt-vue": "^4.7.0",
        "apprt-vuetify": "^4.7.0"
    },
    "layer": "module",
    "i18n": [
        "bundle"
    ],
    "layout-widgets": [
        {
            "widgetRole": "coordinatesWidget",
            "window": {
                "title": "DeepFrAppWindow",
                "dockTool": "coordinatesToggleTool",
                "marginBox": {
                    "l": 50,
                    "b": 150,
                    "w": 300,
                    "h": 500
                },
                "minimizeOnClose": true
            }
        },
        {
            "widgetRole": "weatherWidget",
            "window": {
                "title": "DeepFrAppWeatherWindow",
                "dockTool": "weatherToggleTool",
                "marginBox": {
                    "l": 300,
                    "b": 120,
                    "w": 300,
                    "h": 420
                },
                "closable": false
            }
        }
    ],
    "CSS-Themes-Extension": [],
    "components": [
        {
            "name": "deepfrappFactory",
            "provides": [
                "dijit.Widget"
            ],
            "instanceFactory": true,
            "properties": {
                "widgetRole": "coordinatesWidget"
            },
            "references": [
                {
                    "name": "coordinatesController",
                    "providing": "coordinates.controller"
                }
            ]
        },
        {
            "name": "coordinatesToggleTool",
            "impl": "ct/tools/Tool",
            "provides": "ct.tools.Tool",
            "propertiesConstructor": true,
            "properties": {
                "id": "coordinatesToggleTool",
                "title": "CoordinatesToolTitel",
                "toolRole": "toolset",
                "togglable": true,
                "rules": {
                    "noGroup": true
                },
                "deactivateHandler": "connectClosings"
            },
            "references": [
                {
                    "name": "handlerScope",
                    "providing": "weather.weathercontroller"
                }
            ]
        },
        {
            "name": "deepfrappController",
            "propertiesConstructor": true,
            "provides": "coordinates.controller",
            "properties": {
                "id": "coordinatesController"
            },
            "references": [
                {
                    "name": "_mapWidgetModel",
                    "providing":"map-widget.MapWidgetModel"
                },
                {
                    "name": "coordinatesToggleTool",
                    "providing": "ct.tools.Tool",
                    "filter": "(id=coordinatesToggleTool)"
                },
                {
                    "name": "weathercontroller",
                    "providing": "weather.weathercontroller"
                }
            ]
        },
        {
            "name": "deepfrappweatherFactory",
            "provides": [
                "dijit.Widget"
            ],
            "instanceFactory": true,
            "properties": {
                "widgetRole": "weatherWidget"
            },
            "references": [
                {
                    "name": "weathercontroller",
                    "providing": "weather.weathercontroller"
                }
            ]
        },
        {
            "name": "weatherToggleTool",
            "impl": "ct/tools/Tool",
            "provides": "ct.tools.Tool",
            "propertiesConstructor": true,
            "properties": {
                "id": "weatherToggleTool",
                "title": "weatherToolTitel",
                "toolRole": "toolset",
                "togglable": true,
                "rules": {
                    "noGroup": true
                }
            }
        },
        {
            "name": "deepfrappweatherController",
            "propertiesConstructor": true,
            "provides": "weather.weathercontroller",
            "properties": {
                "id": "weatherController"
            },
            "references": [
                {
                    "name": "weatherToggleTool",
                    "providing": "ct.tools.Tool",
                    "filter": "(id=weatherToggleTool)"
                }
            ]
        }
    ]
}
